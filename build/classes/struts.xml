<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

<constant name="struts.enable.DynamicMethodInvocation" value="false" />
<constant name="struts.devMode" value="true" />

    <package name="Login Package" extends="struts-default" namespace="/">
    
        <action name="Login" class="com.myapp.login.LoginAction" >
            <result name="success">Dashboards/Welcome.jsp</result>
            <result name="input">Login.jsp</result>
        </action>
        
        <action name="adminLogin" class="com.myapp.login.AdminLoginAction" >
            <result name="success">Dashboards/WelcomeAdmin.jsp</result>
            <result name="input">AdminLogin.jsp</result>
        </action>
        
         <action name="Registration" class="com.myapp.accountsManagement.RegistrationAction" >
            <result name="success">Dashboards/Welcome.jsp</result>
            <result name="input">Login.jsp</result>
        </action>
        
        
        <action name="ChildRegistration" class="com.pronatation.Child.ChildRegistrationAction" >
            <result name="success">Dashboards/Welcome.jsp</result>
        </action>

				<action name="viewSession" class="com.pronatation.Session.ViewSessionAction">
						<result name="success">../CreationSession/CreateSession.jsp</result>
				</action>
        
    </package>
    
</struts>
